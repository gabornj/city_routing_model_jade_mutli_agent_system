    public void clearFrame()
    {
        this.repaint();
        mainAg.firstTime=true;    // for the purpose of that when you press 'Clear Environment' menu item or 'Ctrl+R' after that one time draw a point with name and then its line .
    }

    public void updateFrame()
    {
        if(!mainAg.linearMode)
        {
            this.update(getGraphics());
        }
    }

    public void createFunction()
    {
        CreateAgentsFrame caf=new CreateAgentsFrame();
        caf.mainAg=mainAg;
        caf.show();
    }
    
    public void configureFunction()
    {
        ConfigFrame cf=new ConfigFrame();
        cf.mainAg=mainAg;
        cf.show();
    }

