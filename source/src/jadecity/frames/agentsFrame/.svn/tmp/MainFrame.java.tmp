    private void jMenuItem8ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jMenuItem8ActionPerformed

        File here=new File(new File(".").getAbsolutePath());
        JFileChooser browse=new JFileChooser(here);
        browse.showSaveDialog(null);
        if(browse.getSelectedFile()==null)
        {
            return ;
        }
        try
        {
            RandomAccessFile raf = new RandomAccessFile(browse.getSelectedFile().getPath(), "rws");
            for(int i=0;i<mainAg.initAgentsData.size();i++)
            {
                raf.write(mainAg.initAgentsData.get(i).toString().getBytes());
                // these two means an Enter
                raf.write(13);
                raf.write(10);
            }
            raf.close();
        }
        catch (FileNotFoundException ex)
        {
            JOptionPane.showMessageDialog(null,"Error In Saving Init Data ..."+ex.getLocalizedMessage(),"ERROR",JOptionPane.ERROR_MESSAGE);
        }
        catch (IOException ex)
        {
            JOptionPane.showMessageDialog(null,"Error In Saving Init Data ..."+ex.getLocalizedMessage(),"ERROR",JOptionPane.ERROR_MESSAGE);
        }
    }//GEN-LAST:event_jMenuItem8ActionPerformed

