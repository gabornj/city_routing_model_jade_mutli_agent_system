package jadecity.classes.dataStructureClasses;

import jade.core.AID;

/**
 *
 * @author SONY
 */
public class InitData
{
    public Point firstLocation;   // source
    public Point destination;
    public double speed;
    public AID name;


    public InitData()
    {
    }

    public InitData(Point firstLocation, Point destination, double speed,AID name)
    {
        this.firstLocation = firstLocation;
        this.destination = destination;
        this.speed = speed;
        this.name=name;
    }

    @Override
    public String toString()
    {
        return firstLocation.x+","+firstLocation.y+","+speed+","+destination.x+","+destination.y;
    }
}
