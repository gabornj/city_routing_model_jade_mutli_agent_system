package jadecity.classes.dataStructureClasses;

/**
 *
 * @author SONY
 */
public class Report
{
    public Point firstLocation;   // source
    public Point destination;
    public double speed;
    public long durationTime;
    public double totalDistance;
    public String name;

    public Report()
    {
    }

    public Report(String name, Point firstLocation, Point destination, double speed, long durationTime, double totalDistance)
    {
        this.name=name;
        this.firstLocation = firstLocation;
        this.destination = destination;
        this.speed = speed;
        this.durationTime = durationTime;
        this.totalDistance = totalDistance;
    }
}
